{
  "name": "poem-machine",
  "version": "1.0.0",
  "description": "A web app for exploring a poet's corpus",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "clean": "rimraf dist",
    "type-check": "tsc -p ./tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w --hot",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js --debug\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SudoNhim/poem-machine.git"
  },
  "engines": {
    "node": "10.x"
  },
  "author": "Brenton Milne & Gabriela Chuchro",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/body-parser": "^1.19.0",
    "body-parser": "^1.19.0",
    "cohen-db": "github:SudoNhim/cohen-db#9fea4fc",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lunr": "^2.3.9",
    "mongodb": "^3.6.2",
    "mongodb-memory-server": "^6.9.2",
    "mongoose": "^5.10.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lunr": "^2.3.3",
    "@types/mongodb": "^3.5.28",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.68",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/passport-local-mongoose": "^4.0.13",
    "@types/react": "^16.9.53",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "import-sort-style-module": "^6.0.0",
    "markdown-react-js": "^1.0.2",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-scroll-memory": "github:SudoNhim/react-router-scroll-memory",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "ts-node": "^8.10.2",
    "tsc-watch": "^2.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
